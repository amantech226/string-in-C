
#include <stdio.h>
#include<string.h>

int main()
{
  char str[20];
  char temp;
    int i=0,sp=0,j=0,len=0;
    
   printf("Enter a string : ");
    gets(str);
    
    len=strlen(str);
    
    for(i=0,j=len-1;i<=j;i++,j--)
    {
        temp=str[j];
        str[j]=str[i];
        str[i]=temp;
        
    }
    
    puts(str);
    
    
    
    
  
     
    
    return 0;
}
