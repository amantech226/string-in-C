#include <stdio.h>

int main()
{
    char str[20];
    int i=0,sp=0;
    
   printf("Enter a string : ");
    gets(str);
     
     for(i=0;str[i]!='\0';i++)
     {
         if(str[i]==32)
         {
             sp++;
         }
         
     }
     
     
     printf("words:- %d\n",sp+1);

    return 0;
}
